{
  "expo": {
    "name": "صقر",
    "slug": "saqr-app",
    "version": "4.5.0",
    "orientation": "portrait",
    "icon": "./assets/logo_saqr.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/logo_saqr.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0f"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saqr.rewards",
      "buildNumber": "4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "يستخدم التطبيق الكاميرا لتحميل الصور",
        "NSPhotoLibraryUsageDescription": "يستخدم التطبيق مكتبة الصور لتحميل الصور",
        "NSUserTrackingUsageDescription": "يستخدم هذا التطبيق معرّف الإعلان لتقديم إعلانات مخصصة",
        "GADApplicationIdentifier": "ca-app-pub-5132559433385403~6910358704"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_saqr.png",
        "backgroundColor": "#0a0a0f"
      },
      "package": "com.saqr.rewards",
      "versionCode": 30,
      "permissions": ["INTERNET", "VIBRATE"],
      "blockedPermissions": ["android.permission.RECORD_AUDIO"],
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-5132559433385403~6910358704"
      }
    },
    "web": {
      "favicon": "./assets/logo_saqr.png"
    },
    "owner": "ziyad333",
    "extra": {
      "eas": {
        "projectId": "2a08ac90-1772-4377-bf99-af5d2357db30"
      }
    }
  }
}
