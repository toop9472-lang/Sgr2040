{
  "expo": {
    "name": "صقر",
    "slug": "saqr-app",
    "version": "4.1.0",
    "orientation": "portrait",
    "icon": "./assets/logo_saqr.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/logo_saqr.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0f"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24
          }
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saqr.rewards",
      "buildNumber": "4.1.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_saqr.png",
        "backgroundColor": "#0a0a0f"
      },
      "package": "com.saqr.rewards",
      "versionCode": 25,
      "permissions": ["INTERNET", "VIBRATE"],
      "blockedPermissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./assets/logo_saqr.png"
    },
    "owner": "ziyad333",
    "extra": {
      "eas": {
        "projectId": "2a08ac90-1772-4377-bf99-af5d2357db30"
      }
    }
  }
}
